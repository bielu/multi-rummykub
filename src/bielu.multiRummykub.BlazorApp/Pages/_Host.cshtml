@using bielu.multiRummykub.BlazorApp.Components
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <link href="dist/main.css" rel="stylesheet"  asp-append-version="true"/>
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered"/>
</head>
<body>


<component type="typeof(Navigation)" render-mode="ServerPrerendered"/>

