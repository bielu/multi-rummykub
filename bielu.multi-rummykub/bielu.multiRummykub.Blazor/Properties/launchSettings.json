{
  "iisSettings": {
    "iisExpress": {
      "applicationUrl": "http://localhost:42803",
      "sslPort": 44371
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5199",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "jsWebView2Debugging": true,
      "hotReloadEnabled": true,
      "hotReloadProfile": "aspnetcore",
      "applicationUrl": "https://localhost:7130;http://localhost:5199",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Watch": {

      "commandName": "Executable",

      "workingDirectory": "$(ProjectDir)",

      "executablePath": "dotnet.exe",

      "commandLineArgs": "watch run debug --launch-profile Debug"

    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
